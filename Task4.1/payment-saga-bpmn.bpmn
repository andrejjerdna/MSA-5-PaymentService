<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://www.omg.org/spec/BPMN/20100524/MODEL" exporter="Camunda Modeler" exporterVersion="5.43.1">
  <bpmn:process id="PaymentSagaProcess" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:serviceTask id="CreatePaymentOrder" name="CREATE_PAYMENT_ORDER Reserve funds">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-payment-order" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="DebitFunds" name="DEBIT_FUNDS Debit reserved funds">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="debit-funds" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="AntifraudCheck" name="ANTIFRAUD_CHECK Fraud analysis">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="antifraud-check" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:exclusiveGateway id="Gateway_FraudResult">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejected</bpmn:outgoing>
      <bpmn:outgoing>Flow_ManualReview</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:sequenceFlow id="Flow_Approved" sourceRef="Gateway_FraudResult" targetRef="PayoutToContragent">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="feel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">=fraudResult = "APPROVED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_Rejected" sourceRef="Gateway_FraudResult" targetRef="CompensateDebit">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="feel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">=fraudResult = "REJECTED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_ManualReview" sourceRef="Gateway_FraudResult" targetRef="WaitManualReview">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="feel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">=fraudResult = "MANUAL_REVIEW"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:serviceTask id="WaitManualReview" name="WAIT_FOR_MANUAL_REVIEW (max 20 min)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="wait-manual-review" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ManualReview</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:exclusiveGateway id="Gateway_ManualDecision">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_ManualApproved</bpmn:outgoing>
      <bpmn:outgoing>Flow_ManualRejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:sequenceFlow id="Flow_5" sourceRef="WaitManualReview" targetRef="Gateway_ManualDecision" />
    
    <bpmn:sequenceFlow id="Flow_ManualApproved" sourceRef="Gateway_ManualDecision" targetRef="PayoutToContragent">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="feel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">=manualDecision = "APPROVED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_ManualRejected" sourceRef="Gateway_ManualDecision" targetRef="CompensateDebit">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="feel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">=manualDecision = "REJECTED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:serviceTask id="PayoutToContragent" name="PAYOUT_TO_CONTRAGENT Transfer to merchant">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="transfer-to-merchant" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Approved</bpmn:incoming>
      <bpmn:incoming>Flow_ManualApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="CompensateDebit" name="COMPENSATE_DEBIT_FUNDS Refund to user">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="refund-amount" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Rejected</bpmn:incoming>
      <bpmn:incoming>Flow_ManualRejected</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="NotifySuccess" name="NOTIFY_CUSTOMER Payment successful">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="send-notice" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_EndSuccess</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="NotifyFailed" name="NOTIFY_CUSTOMER Payment declined">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="send-notice" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_EndFailed</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:endEvent id="EndSuccess">
      <bpmn:incoming>Flow_EndSuccess</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="EndFailed">
      <bpmn:incoming>Flow_EndFailed</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="CreatePaymentOrder" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="CreatePaymentOrder" targetRef="DebitFunds" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="DebitFunds" targetRef="AntifraudCheck" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="AntifraudCheck" targetRef="Gateway_FraudResult" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="PayoutToContragent" targetRef="NotifySuccess" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="CompensateDebit" targetRef="NotifyFailed" />
    <bpmn:sequenceFlow id="Flow_EndSuccess" sourceRef="NotifySuccess" targetRef="EndSuccess" />
    <bpmn:sequenceFlow id="Flow_EndFailed" sourceRef="NotifyFailed" targetRef="EndFailed" />
  </bpmn:process>
  
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="PaymentSagaProcess">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="150" y="260" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="CreatePaymentOrder_di" bpmnElement="CreatePaymentOrder">
        <dc:Bounds x="250" y="240" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="DebitFunds_di" bpmnElement="DebitFunds">
        <dc:Bounds x="420" y="240" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="AntifraudCheck_di" bpmnElement="AntifraudCheck">
        <dc:Bounds x="580" y="240" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_FraudResult_di" bpmnElement="Gateway_FraudResult" isMarkerVisible="true">
        <dc:Bounds x="735" y="253" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="WaitManualReview_di" bpmnElement="WaitManualReview">
        <dc:Bounds x="758" y="500" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_ManualDecision_di" bpmnElement="Gateway_ManualDecision" isMarkerVisible="true">
        <dc:Bounds x="1025" y="465" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="CompensateDebit_di" bpmnElement="CompensateDebit">
        <dc:Bounds x="860" y="80" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="PayoutToContragent_di" bpmnElement="PayoutToContragent">
        <dc:Bounds x="1020" y="240" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="NotifySuccess_di" bpmnElement="NotifySuccess">
        <dc:Bounds x="1180" y="240" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="NotifyFailed_di" bpmnElement="NotifyFailed">
        <dc:Bounds x="1180" y="80" width="120" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndSuccess_di" bpmnElement="EndSuccess">
        <dc:Bounds x="1372" y="239" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndFailed_di" bpmnElement="EndFailed">
        <dc:Bounds x="1372" y="79" width="36" height="36" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="186" y="278" />
        <di:waypoint x="250" y="278" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="370" y="278" />
        <di:waypoint x="420" y="278" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="540" y="278" />
        <di:waypoint x="580" y="278" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="700" y="278" />
        <di:waypoint x="735" y="278" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Approved_di" bpmnElement="Flow_Approved">
        <di:waypoint x="785" y="278" />
